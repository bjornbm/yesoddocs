#map, #topics, #labels {
    display: table-cell;
    padding-right: 5em;
}

li.node, li.dropzone {
    cursor: pointer;
}

li.selected > span {
    color: #900;
}

li.dropzone {
    background-color: #090;
    font-size: 0.8em;
}

li.dropzone.hidden {
    background-color: #efe;
    color: #efe;
}
